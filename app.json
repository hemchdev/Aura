{
  "expo": {
    "name": "Aura",
    "slug": "auraai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aura",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pp.aura",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.pp.aura",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png"
      },
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM"
      ],
      "edgeToEdgeEnabled": false,
      "blockedPermissions": [],
      "allowBackup": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#8B5CF6"
        }
      ],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "This app uses the microphone to enable voice commands and speech recognition.",
          "speechRecognitionPermission": "This app uses speech recognition to process voice commands."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so",
                "**/libjsc.so"
              ],
              "exclude": [
                "**/META-INF/androidx.localbroadcastmanager_localbroadcastmanager.version",
                "**/META-INF/androidx.customview_customview.version",
                "**/META-INF/androidx.core_core.version",
                "**/META-INF/androidx.lifecycle_lifecycle-runtime.version",
                "**/META-INF/androidx.savedstate_savedstate.version",
                "**/META-INF/androidx.legacy_legacy-support-core-ui.version",
                "**/META-INF/androidx.legacy_legacy-support-core-utils.version",
                "**/META-INF/androidx.fragment_fragment.version",
                "**/META-INF/androidx.activity_activity.version",
                "**/META-INF/androidx.appcompat_appcompat.version",
                "**/META-INF/androidx.drawerlayout_drawerlayout.version",
                "**/META-INF/androidx.coordinatorlayout_coordinatorlayout.version",
                "**/META-INF/androidx.recyclerview_recyclerview.version",
                "**/META-INF/androidx.viewpager_viewpager.version",
                "**/META-INF/androidx.loader_loader.version",
                "**/META-INF/androidx.interpolator_interpolator.version",
                "**/META-INF/androidx.cursoradapter_cursoradapter.version",
                "**/META-INF/androidx.swiperefreshlayout_swiperefreshlayout.version",
                "**/META-INF/androidx.slidingpanelayout_slidingpanelayout.version",
                "**/META-INF/androidx.slidingpanelayout_slidingpanelayout.version"
              ]
            }
          }
        }
      ],
      "./plugins/withAndroidManifestFix.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d9636115-c728-4fed-9aad-c5417f8ef898"
      }
    },
    "owner": "trailapp2"
  }
}
